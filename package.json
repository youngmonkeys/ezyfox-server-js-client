{
  "name": "ezyfox-js-client",
  "version": "1.0.4",
  "description": "EzyFox Server's javascript client",
  "scripts": {
    "test": "test",
    "lint": "eslint \"**/*.{js,ts,tsx}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/youngmonkeys/ezyfox-server-js-client.git"
  },
  "keywords": [
    "ezyfox"
  ],
  "author": "tvd12",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/youngmonkeys/ezyfox-server-js-client/issues"
  },
  "homepage": "https://github.com/youngmonkeys/ezyfox-server-js-client#readme",
  "dependencies": {},
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "commitlint": "^11.0.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 4,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "es5",
    "useTabs": false
  }
}
